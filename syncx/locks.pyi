"""Wrappers around parking_lot locks with Python-friendly names."""

from typing import Optional

class Lock:
    def __init__(self) -> None: ...
    def acquire(self, blocking: bool = ..., timeout: Optional[float] = ...) -> bool: ...
    def lock(self, blocking: bool = ..., timeout: Optional[float] = ...) -> bool: ...
    def try_acquire(self) -> bool: ...
    def try_lock(self) -> bool: ...
    def release(self) -> None: ...
    def locked(self) -> bool: ...
    def is_locked(self) -> bool: ...
    def guard(self, blocking: bool = ..., timeout: Optional[float] = ...) -> Optional["LockGuard"]: ...
    def __enter__(self) -> "Lock": ...
    def __exit__(self, exc_type, exc, tb) -> bool: ...

class LockGuard:
    def release(self) -> None: ...
    def unlock(self) -> None: ...
    def __enter__(self) -> "LockGuard": ...
    def __exit__(self, exc_type, exc, tb) -> bool: ...

class RLock:
    def __init__(self) -> None: ...
    def acquire(self) -> "RLockGuard": ...
    def lock(self) -> "RLockGuard": ...
    def try_acquire(self) -> Optional["RLockGuard"]: ...
    def try_lock(self) -> Optional["RLockGuard"]: ...
    def __enter__(self) -> "RLockGuard": ...
    def __exit__(self, exc_type, exc, tb) -> bool: ...

class RLockGuard:
    def release(self) -> None: ...
    def unlock(self) -> None: ...
    def __enter__(self) -> "RLockGuard": ...
    def __exit__(self, exc_type, exc, tb) -> bool: ...

class RWLock:
    def __init__(self) -> None: ...
    def acquire_read(self, blocking: bool = ..., timeout: Optional[float] = ...) -> bool: ...
    def read_lock(self, blocking: bool = ..., timeout: Optional[float] = ...) -> bool: ...
    def read_release(self) -> None: ...
    def try_acquire_read(self) -> bool: ...
    def try_read_lock(self) -> bool: ...
    def read_guard(self, blocking: bool = ..., timeout: Optional[float] = ...) -> Optional["ReadGuard"]: ...
    def acquire_write(self, blocking: bool = ..., timeout: Optional[float] = ...) -> bool: ...
    def write_lock(self, blocking: bool = ..., timeout: Optional[float] = ...) -> bool: ...
    def write_release(self) -> None: ...
    def write_guard(self, blocking: bool = ..., timeout: Optional[float] = ...) -> Optional["WriteGuard"]: ...
    def try_acquire_write(self) -> bool: ...
    def try_write_lock(self) -> bool: ...
    def is_locked(self) -> bool: ...
    def is_write_locked(self) -> bool: ...
    def write_release_fair(self) -> None: ...
    def bump_shared(self) -> None: ...
    def bump_exclusive(self) -> None: ...

class ReadGuard:
    def release(self) -> None: ...
    def unlock(self) -> None: ...
    def __enter__(self) -> "ReadGuard": ...
    def __exit__(self, exc_type, exc, tb) -> bool: ...

class WriteGuard:
    def release(self) -> None: ...
    def unlock(self) -> None: ...
    def release_fair(self) -> None: ...
    def unlock_fair(self) -> None: ...
    def downgrade(self) -> Optional[ReadGuard]: ...
    def __enter__(self) -> "WriteGuard": ...
    def __exit__(self, exc_type, exc, tb) -> bool: ...

__all__ = [
    "Lock",
    "LockGuard",
    "RLock",
    "RLockGuard",
    "RWLock",
    "ReadGuard",
    "WriteGuard",
]
